<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>save_data</servlet-name>
        <servlet-class>EID.save_data</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkinternet</servlet-name>
        <servlet-class>EID.checkinternet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_data</servlet-name>
        <servlet-class>EID.delete_data</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>save_users</servlet-name>
        <servlet-class>EID.save_users</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>copytemplates</servlet-name>
        <servlet-class>code.copytemplates</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>load_counties</servlet-name>
        <servlet-class>loaders.load_counties</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_district</servlet-name>
        <servlet-class>loaders.load_district</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_facilities</servlet-name>
        <servlet-class>loaders.load_facilities</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RawData</servlet-name>
        <servlet-class>Reports.RawData</servlet-class>
    </servlet>
    
    
     <servlet-mapping>
        <servlet-name>copytemplates</servlet-name>
        <url-pattern>/copytemplates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save_data</servlet-name>
        <url-pattern>/save_data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_data</servlet-name>
        <url-pattern>/delete_data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkinternet</servlet-name>
        <url-pattern>/checkinternet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save_users</servlet-name>
        <url-pattern>/save_users</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_counties</servlet-name>
        <url-pattern>/load_counties</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_district</servlet-name>
        <url-pattern>/load_district</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_facilities</servlet-name>
        <url-pattern>/load_facilities</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RawData</servlet-name>
        <url-pattern>/RawData</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
